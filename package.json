  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "codecov": "^3.1.0",
    "jest": "^23.6.0",
    "stryker": "^0.32.1",
    "stryker-api": "^0.21.5",
    "stryker-jest-runner": "^1.2.7",
    "stryker-typescript": "^0.15.4",
    "ts-jest": "^23.10.4",
    "ts-node": "^8.5.0",
    "typedoc": "^0.17.5",
    "typescript": "^3.3.1"
  },
  "main": "dist/index.js",
  "files": [
    "/dist"
  ],
  "dependencies": {},
  "jest": {
    "coverageDirectory": "./coverage/",
